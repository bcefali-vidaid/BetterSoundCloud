{
  "manifest_version": 2,

  "name": "Better SoundCloud",
  "description": "An extension to make soundcloud better",
  "version": "1.0",

  "content_scripts": [{
    "matches": ["*://soundcloud.com/*"],
	"css": ["better_sc.css"],
	"js": ["sdk-3.1.2.js", "better_sc.js"]
  }],
  
  "options_page": "options.html",
  
  "permissions": [
    "activeTab",
	"storage"
  ]
}